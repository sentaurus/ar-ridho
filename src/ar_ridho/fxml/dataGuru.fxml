<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" minHeight="645.0" minWidth="1200.0" styleClass="mainFxmlClass"
            stylesheets="@../css/dataguru.css" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ar_ridho.kontroller.DataGuruController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Data Guru" textFill="WHITE">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        <GridPane layoutX="14.0" layoutY="35.0" minWidth="680.0" prefHeight="160.0" prefWidth="990.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="80.0" prefWidth="80.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="250.0" prefWidth="250.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="80.0" prefWidth="80.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="250.0" prefWidth="250.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="80.0" prefWidth="80.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="250.0" prefWidth="250.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints prefHeight="40.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="NIP" textFill="WHITE"/>
                <Label text="Kode Jabatan" textFill="WHITE" GridPane.rowIndex="1"/>
                <Label text="Nama" textFill="WHITE" GridPane.rowIndex="2"/>
                <TextField fx:id="nip" prefHeight="17.0" prefWidth="266.0" GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="nama" prefHeight="25.0" prefWidth="269.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </TextField>
                <Label text="Jenis Kelamin" textFill="WHITE" GridPane.rowIndex="3"/>
                <ComboBox fx:id="jsKelamin" prefHeight="25.0" prefWidth="163.0" styleClass="combo-box"
                          GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </ComboBox>
                <Label text="Tempat Lahir" textFill="WHITE" GridPane.columnIndex="2">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Label>
                <Label text="Tanggal Lahir" textFill="WHITE" GridPane.columnIndex="2" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Label>
                <Label text="Agama" textFill="WHITE" GridPane.columnIndex="2" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Label>
                <Label text="Status" textFill="WHITE" GridPane.columnIndex="2" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Label>
                <TextField fx:id="tmpLahir" prefHeight="25.0" prefWidth="183.0" GridPane.columnIndex="3">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </TextField>
                <DatePicker fx:id="tglLahir" prefHeight="25.0" prefWidth="190.0" GridPane.columnIndex="3"
                            GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </DatePicker>
                <ComboBox fx:id="status" prefHeight="25.0" prefWidth="187.0" GridPane.columnIndex="3"
                          GridPane.rowIndex="3">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </ComboBox>
                <ComboBox fx:id="agama" prefHeight="25.0" prefWidth="187.0" GridPane.columnIndex="3"
                          GridPane.rowIndex="2">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </ComboBox>
                <Label text="Mulai Kerja" textFill="WHITE" GridPane.columnIndex="4"/>
                <DatePicker fx:id="mlKerja" prefHeight="25.0" prefWidth="190.0" GridPane.columnIndex="5">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </DatePicker>
                <Label text="Kepegawaian" textFill="WHITE" GridPane.columnIndex="4" GridPane.rowIndex="1"/>
                <ComboBox fx:id="kepegawai" prefHeight="25.0" prefWidth="230.0" GridPane.columnIndex="5"
                          GridPane.rowIndex="1">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </ComboBox>
                <Label text="Alamat" textFill="WHITE" GridPane.columnIndex="4" GridPane.rowIndex="2"/>
                <TextArea fx:id="alamat" prefHeight="37.0" prefWidth="101.0" wrapText="true" GridPane.columnIndex="5"
                          GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </TextArea>
                <HBox prefHeight="90.0" prefWidth="279.0" GridPane.columnIndex="5" GridPane.rowIndex="3">
                    <children>
                        <Button fx:id="simpan" minWidth="70.0" mnemonicParsing="false" onAction="#onSimpan"
                                text="simpan">
                            <HBox.margin>
                                <Insets right="10.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="ubah" minWidth="70.0" mnemonicParsing="false" onAction="#onUbah" text="ubah">
                            <HBox.margin>
                                <Insets right="10.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="hapus" minWidth="70.0" mnemonicParsing="false" onAction="#onHapus" text="hapus">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </Button>
                    </children>
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </HBox>
                <TextField fx:id="kdJabatan" prefHeight="25.0" prefWidth="162.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </GridPane.margin>
                </TextField>
            </children>
        </GridPane>
        <TableView fx:id="tabel" layoutX="25.0" layoutY="207.0" minWidth="1150.0" onMouseClicked="#onKlik"
                   prefHeight="424.0" prefWidth="1150.0">
            <columns>
                <TableColumn fx:id="tNip" prefWidth="75.0" text="NIP"/>
                <TableColumn fx:id="tKdJabatan" prefWidth="75.0" text="Kode Jabatan"/>
                <TableColumn fx:id="tNama" prefWidth="75.0" text="Nama"/>
                <TableColumn fx:id="tJsKelamin" prefWidth="75.0" text="Jenis Kelamin"/>
                <TableColumn fx:id="tTmpLahir" prefWidth="75.0" text="Tempat Lahir"/>
                <TableColumn fx:id="tTglLahir" prefWidth="75.0" text="Tanggal Lahir"/>
                <TableColumn fx:id="tAgama" prefWidth="75.0" text="Agama"/>
                <TableColumn fx:id="tAlamat" prefWidth="75.0" text="Alamat"/>
                <TableColumn fx:id="tStatus" prefWidth="75.0" text="Status"/>
                <TableColumn fx:id="tMlKerja" prefWidth="75.0" text="Mulai Kerja"/>
                <TableColumn fx:id="tKepegawai" prefWidth="75.0" text="Kepegawaian"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</AnchorPane>
